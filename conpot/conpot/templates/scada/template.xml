<core>
    <template>
        <!-- General information about the template -->
        <entity name="unit">SCADA System</entity>
        <entity name="vendor">Generic Vendor</entity>
        <entity name="description">simulacion SCADA con los protocolos HTTP y SNMP </entity>
        <entity name="protocols">HTTP, SNMP</entity>
        <entity name="creator">SCADA_BY_HAIZEA</entity>
    </template>
    <databus>
        <!-- Core value that can be retrieved from the databus by key -->
        <key_value_mappings>
            <key name="FacilityName">
                <value type="value">"Simulated SCADA Facility"</value>
            </key>
            <key name="SystemName">
                <value type="value">"SCADA System"</value>
            </key>
            <key name="SystemDescription">
                <value type="value">"SCADA, HTTP, SNMP Monitoring"</value>
            </key>
            <key name="Uptime">
                <value type="function">conpot.emulators.misc.uptime.Uptime</value>
            </key>
            <key name="sysObjectID">
                <value type="value">"0.0"</value>
            </key>
            <key name="sysContact">
                <value type="value">"SCADA Operator"</value>
            </key>
            <key name="sysName">
                <value type="value">"SCADA Monitoring Unit"</value>
            </key>
            <key name="sysLocation">
                <value type="value">"Main SCADA Facility"</value>
            </key>
            <key name="sysServices">
                <value type="value">"72"</value>
            </key>
            <key name="empty">
                <value type="value">""</value>
            </key>
        </key_value_mappings>
    </databus>
</core>
